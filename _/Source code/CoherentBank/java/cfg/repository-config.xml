<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="accountRepository"
        class="com.seovic.samples.bank.domain.repository.CoherenceAccountRepository"/>

  <bean id="customerRepository"
        class="com.seovic.samples.bank.domain.repository.CoherenceCustomerRepository"/>

  <bean id="exchangeRateRepository"
        class="com.seovic.samples.bank.domain.repository.CoherenceExchangeRateRepository"/>

  <bean id="transactionRepository"
        class="com.seovic.samples.bank.domain.repository.CoherenceTransactionRepository"/>

</beans>